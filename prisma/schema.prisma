generator client {
  provider = "prisma-client-js"
}

generator json {
  provider = "prisma-json-types-generator"
  // namespace = "PrismaJson"
  // clientOutput = "<finds it automatically>"
  // (./ -> relative to schema, or an importable path to require() it)
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

enum Status {
  active
  prospect
  archived
}

enum Role {
  ADMIN
  COLLABORATOR
  CLIENT
}

enum VisaType {
  renovacao
  primeiro_visto
}

enum VisaClass {
  B1
  B2_B1
  O1
  O2
  O3
}

enum VisaStatus {
  approved
  disapproved
  awaiting
  in_progress
  finished
}

enum StatusDS {
  filled
  filling
  awaiting
  emitted
}

enum ScheduleAccount {
  active
  inactive
}

enum BudgetPaid {
  paid
  pending
}

enum StatusForm {
  awaiting
  filling
  filled
}

enum NotificationStatusForm {
  filling
  filled
  updated
}

enum Category {
  american_visa
  passport
  e_ta
}

model User {
  id                      String           @id @default(auto()) @map("_id") @db.ObjectId
  name                    String
  email                   String           @unique
  password                String
  role                    Role
  group                   String?
  emailVerified           DateTime?
  image                   String?
  budget                  Float?
  budgetPaid              BudgetPaid?
  cpf                     String?
  cel                     String?
  address                 String?
  emailScheduleAccount    String?
  passwordScheduleAccount String?
  scheduleAccount         ScheduleAccount?

  profiles    Profile[]
  annotations Annotations[]
  accounts    Account[]
  sessions    Session[]
  comments    Comments[]

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model Profile {
  id            String     @id @default(auto()) @map("_id") @db.ObjectId
  name          String
  birthDate     DateTime?
  cpf           String?
  address       String?
  CASVDate      DateTime?
  interviewDate DateTime?
  interviewTime String?
  visaType      VisaType
  visaClass     VisaClass
  visaStatus    VisaStatus @default(awaiting)
  statusDS      StatusDS   @default(awaiting)
  DSNumber      String
  DSValid       DateTime
  passport      String?
  taxDate       DateTime?
  meetingDate   DateTime?
  issuanceDate  DateTime?
  expireDate    DateTime?
  statusForm    StatusForm @default(awaiting)
  formStep      Int        @default(0)
  category      Category
  status        Status     @default(active)

  userId       String         @db.ObjectId
  user         User           @relation(fields: [userId], references: [id], onDelete: Cascade)
  form         Form?
  comments     Comments[]
  notification Notification[]

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model Account {
  id                String  @id @default(auto()) @map("_id") @db.ObjectId
  userId            String  @db.ObjectId
  type              String
  provider          String
  providerAccountId String
  refresh_token     String? @db.String
  access_token      String? @db.String
  expires_at        Int?
  token_type        String?
  scope             String?
  id_token          String? @db.String
  session_state     String?

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  user User @relation(fields: [userId], references: [id], onDelete: Cascade)

  @@unique([provider, providerAccountId])
}

model Session {
  id           String   @id @default(auto()) @map("_id") @db.ObjectId
  sessionToken String   @unique
  userId       String   @db.ObjectId
  expires      DateTime
  user         User     @relation(fields: [userId], references: [id], onDelete: Cascade)

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

