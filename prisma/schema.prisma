  formId String @db.ObjectId
  form   Form   @relation(fields: [formId], references: [id], onDelete: Cascade)
}

model Course {
  id              String    @id @default(auto()) @map("_id") @db.ObjectId
  institutionName String?
  address         String?
  city            String?
  state           String?
  country         String?
